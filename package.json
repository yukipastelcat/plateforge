    "@types/node": "^18",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vitest": "^0.2.2",
    "husky": "^8.0.3",
    "nuxt": "^3.4.3",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "vitest": "^0.30.1"
  }
